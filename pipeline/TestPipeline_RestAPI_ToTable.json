{
	"name": "TestPipeline_RestAPI_ToTable",
	"properties": {
		"activities": [
			{
				"name": "COPY_RestAPI_ADLS",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET"
					},
					"sink": {
						"type": "AzureSqlSink",
						"tableOption": "autoCreate"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['status']"
								},
								"sink": {
									"name": "status",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['id']"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['employee_name']"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['employee_salary']"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['employee_age']"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['profile_image']"
								}
							},
							{
								"source": {
									"path": "$['message']"
								},
								"sink": {
									"name": "message",
									"type": "String"
								}
							}
						],
						"collectionReference": "",
						"mapComplexValuesToString": true
					}
				},
				"inputs": [
					{
						"referenceName": "RestResource1",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AzureSqlTable1",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}